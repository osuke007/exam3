<div class="container">
  <h2>ユーザー</h2>
  <% @users.each do |user| %>
    <% if current_user.id != user.id %>
      <div class="container-child col-md-2 col-sm-4 col-xs-6 ">
        <%= profile_img(user) %><br>
        <%= user.email %><br>
        <%= user.name %><br>
        <%= link_to 'メッセージ', conversations_path(sender_id: current_user.id, recipient_id: user.id), method: 'post'%><br>
        <%= render partial: 'follow_form', locals: { user: user } if signed_in?   %>
        <hr>
      </div>
    <% end %>
  <% end %>



</div>
